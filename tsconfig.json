{
  "compilerOptions": {
    "jsx": "preserve",
    "esModuleInterop": true,
    "strict": false,
    "strictBindCallApply": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "allowJs": true,
    "checkJs": true,
    "outDir": "types",
    "declaration": true,
    "emitDeclarationOnly": true
  },
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
